{
	"compilerOptions": {
		"target": "es2017",
		"module": "esnext",
		"outDir": "dist",
		"moduleResolution": "node",
		"sourceMap": true,
		"strict": true,
		"esModuleInterop": true,
		"allowJs": true,
		"lib": [
			"es6"
		],
	},
	"include": [
		"src/**/*"
	],
	"exclude": [
		"./dist",
		"./node_modules"
	],
}